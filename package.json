{
  "name": "predictive",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "transpile": "tsc",
    "start": "LOCAL_RUN=true LD_LIBRARY_PATH=./lib tsc-watch --onSuccess \"node build/runLambda.js\"",
    "test": "JUNIT_REPORT_PATH=build/reports/unit/junit.xml nyc mocha",
    "package": "copyfiles package.json lib/lib*.so* build; copy-node-modules . ./build; cd build && zip -r ../lambda.zip *",
    "compose:up": "./run.sh",
    "compose:down": "docker-compose down"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.318.0",
    "oracledb": "^2.3.0",
    "oracledb-for-lambda": "^1.9.3-8a",
    "sleep": "^5.2.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.13",
    "@types/aws-sdk": "^2.7.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.10.1",
    "@types/oracledb": "^1.11.34",
    "@types/sleep": "0.0.7",
    "assert": "^1.4.1",
    "copy-node-modules": "^1.0.8",
    "copyfiles": "^2.1.0",
    "mocha": "^5.2.0",
    "mocha-jenkins-reporter": "^0.4.0",
    "nyc": "^13.0.1",
    "ts-node": "^7.0.1",
    "tsc-watch": "^1.0.27",
    "typescript": "^3.0.3"
  },
  "nyc": {
    "include": [
      "lib/**/*.ts"
    ],
    "exclude": [
      "typings"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "cobertura"
    ],
    "all": true,
    "sourceMap": true,
    "instrument": true,
    "report-dir": "./build/reports/coverage"
  }
}
